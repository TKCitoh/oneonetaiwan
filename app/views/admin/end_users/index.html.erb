<div class=“container”>
	<h2>ユーザー一覧</h2>
	 <table class="table">
	    <thead>
	      <tr>
	        <th>ユーザーID</th>
	        <th>名前</th>
	        <th>メールアドレス</th>
	        <th>ステータス</th>
	      </tr>
	    </thead>
	    <tbody>
	    	<% @end_users.each do |end_user| %>
	    	 <tr>
          <td><%= end_user.id %></td>
          <td><%= link_to admin_end_user_path(end_user) do %>
              <%= end_user.name %>
              <% end %>
          </td>
          <td><%= end_user.email %></td>
          <td><% if end_user.is_deleted==true %>
              退会
              <% else %>
              有効
              <% end %>
              </td>
         </tr>
        <% end %>
	    </tbody>
	   </table>
	 </h2>
</div>

