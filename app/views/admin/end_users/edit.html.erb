<div class="container">
  <h2><%= @end_user.name %>さんのユーザー情報編集</h2>
	  <div class="col-xs-5">
	    <%= form_with model: @end_user, url: admin_end_user_path, method: :patch do |f| %>
      <div class="field">
        <%= f.label "ユーザーID" %>
        <%= @end_user.id %>
      </div>
　  </div>
　  <div class="col-xs-7">
      <div class="field">
        <%= f.label "名前" %>
        <%= f.text_field :name, autofocus: true %>
      </div>
      <div class="field">
        <%= f.label "メールアドレス" %>
        <%= f.text_field :email, autofocus: true %>
      </div>
      <div class="field">
        <%= f.label "会員ステータス" %>
        <%= f.radio_button :is_deleted, :valid, checked: true %> 有効
        <%= f.radio_button :is_deleted, :withdrawal %> 退会
     </div>
     <%= f.submit "編集内容を保存する", class: "btn btn-primary"%>
     <% end %>
  </div>
</div>